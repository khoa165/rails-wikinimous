<%= form_for (task)  do |f| %>
  <div class="form-group">
    <h6><%= f.label :title %></h6>
    <%= f.text_field :title %>
  </div>
  <div class="form-group">
    <h6><%= f.label :details %></h6>
    <%= f.text_field :details %>
  </div>

  <%= f.submit class: 'btn btn-outline-primary' %>
<% end %>
